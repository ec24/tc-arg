{% extends 'layout.njk' %}

{% block current_url %}2014.html{% endblock %}

{% block main_header %}
  <div class="col-md-10 text-center" data-aos="fade">
    <h1 class="mb-4">Training Camp Argentina 2014</h1>
    <h3 class="mb-5" style="color:white">Universidad de Buenos Aires</h3>
  </div>
{% endblock %}

{% block content %}
  <div class="site-section">
    <div class="container">
      <div class="row">
        <div class="col-md-6 mx-auto text-center mb-5 section-heading">
          <h2 class="mb-5 text-uppercase">Sponsors</h2>
        </div>
      </div>
      <div class="row">
        <div class="col-6 col-md-6 col-lg-4">
          <img src="images/2014/sponsors/sadosky.jpg" alt="Image" class="img-fluid">
        </div>
        <div class="col-6 col-md-6 col-lg-4">
          <img src="images/2014/sponsors/google.jpg" alt="Image" class="img-fluid">
        </div>
        <div class="col-6 col-md-6 col-lg-4">
          <img src="images/2014/sponsors/brightsector.png" alt="Image" class="img-fluid">
        </div>
        <div class="col-6 col-md-6 col-lg-4">
          <img src="images/2014/sponsors/microsoft.png" alt="Image" class="img-fluid">
        </div>
      </div>
    </div>
  </div>

    <div class="site-section">
      <div class="container">
        <div class="row">
          <div class="col-md-7 mx-auto text-center section-heading">
            <h2 class="mb-5 text-uppercase">Clases Teóricas</h2>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-1"></div>
          <div class="col-lg-10">
            <ul>
              <li><a href="pdfs/2014/01-DP.pdf">Programación Dinámica</a></li>
              <li><a href="pdfs/2014/02-Grafos.pdf">Grafos parte I</a></li>
              <li><a href="pdfs/2014/03-BS.pdf">Búsqueda Binaria</a></li>
              <li><a href="pdfs/2014/04-Golosos.pdf">Algoritmos Golosos</a></li>
              <li><a href="pdfs/2014/05-Strings.pdf">Strings</a></li>
              <li><a href="pdfs/2014/06-Grafos2.pdf">Grafos parte II</a></li>
              <li><a href="pdfs/2014/07-Grafos3.pdf">Grafos parte III</a></li>
              <li><a href="pdfs/2014/08-Estructuras.pdf">Estructuras de Datos</a></li>
              <li><a href="pdfs/2014/09-Aritmetica.pdf">Aritmética</a></li>
              <li><a href="pdfs/2014/10-Geometria.pdf">Geometría</a></li>
              <li><a href="pdfs/2014/11-Matematica+FFT.pdf">Matemática (FFT + Extras)</a></li>
              <li><a href="pdfs/2014/12-Flujo.pdf">Flujo</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>

    <div class="site-section">
      <div class="container">
        <div class="row">
          <div class="col-md-7 mx-auto text-center section-heading">
            <h2 class="mb-5 text-uppercase">Oradores</h2>
          </div>
        </div>
      </div>
      <div class="container">
        {% from 'edition_macros.njk' import 'speaker' %}
        {{ speaker('Nicolás Álvarez', 'Licenciado en Ciencias de la Computación de
        la Universidad Nacional del Sur. Estudiante del Doctorado en Ciencias de la
        Computación de la Universidad Nacional del Sur. Finalista como participante
        (2009) y como coach (2010, 2013, 2015) de la ACM ICPC. Jefe de Trabajos
        Prácticos en la Universidad Nacional del Sur.') }}
        {{ speaker('Martín Fixman', 'Estudiante de Licenciatura en Ciencias de la
        Computación de la Facultad de Ciencias Exactas y Naturales, Universidad de
        Buenos Aires. Medalla de Bronce en la 20° Olimpíada Internacional de Informática
        (El Cairo 2008) y Medalla de Plata en la 22° Olimpíada Internacional de
        Informática (Waterloo 2010). Finalista en 2013 de la ACM ICPC. Martín fue
        pasante de Google (USA) y Facebook (USA).') }}
        {{ speaker('Agustín Santiago Gutiérrez' , 'Licenciado en Ciencias de la
        Computación de la Universidad de Buenos Aires. Finalista y campeón
        latinoamericano en dos ocasiones como competidor (2009 y 2011) y finalista
